{
  "name": "kings-league",
  "version": "1.0.0",
  "description": "Kings League Infojobs Project with API and Web",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:api": "wrangler dev api/index.js",
    "publish:api": "wrangler publish api/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint ./**/*.{js,jsx}",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "astro": "1.9.0",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.2",
    "node-fetch": "3.3.0",
    "prettier-plugin-astro": "^0.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.1",
    "eslint-plugin-astro": "0.21.1",
    "prettier": "2.8.2",
    "standard": "17.0.0",
    "vitest": "0.26.3",
    "wrangler": "2.6.2"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended"
    ],
    "rules": {
      "space-before-function-paren": "off"
    }
  },
  "overrides": [
    {
      "files": [
        "*.astro"
      ],
      "parser": "astro-eslint-parser",
      "rules": {
        "astro/no-set-html-directive": "error"
      }
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dver26/kings-league-project.git"
  },
  "bugs": {
    "url": "https://github.com/dver26/kings-league-project/issues"
  },
  "homepage": "https://github.com/dver26/kings-league-project#readme"
}
